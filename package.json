{
  "name": "cyclos4-ui",
  "displayName": "Cyclos 4 UI",
  "description": "A modern and open web interface to Cyclos, which can be easily customized and self-hosted",
  "icon": "cyclos.png",
  "version": "3.0.0-dev",
  "license": "MIT",
  "author": {
    "name": "Cyclos development team",
    "email": "info@cyclos.org",
    "url": "https://www.cyclos.org"
  },
  "scripts": {
    "ng": "ng",
    "ng-openapi-gen": "ng-openapi-gen",
    "ng-translation-gen": "ng-translation-gen",
    "merge-translations": "ng-translation-gen --merge",
    "svgo": "svgo",
    "generate-icons": "node tools/generate-icons.js",
    "generate": "ng-openapi-gen && ng-translation-gen && npm run generate-icons",
    "optimize-svgs": "svgo src/icons/*.*.svg -config='{ \"plugins\": [{ \"removeViewBox\": false }] }'",
    "prepare-build": "npm run generate && npm run && npm run merge-translations",
    "ng-build": "ng build --prod --progress=false",
    "start": "npm run generate && ng serve --host 0.0.0.0 --proxy-config proxy.json",
    "build": "npm run prepare-build && npm run ng-build",
    "analyze": "npm run build -- --stats-json && webpack-bundle-analyzer --port 9200 dist/ui/stats.json",
    "lint": "npm run ng -- lint",
    "pay:start": "npm run generate && ng serve --project pay --host 0.0.0.0 --proxy-config proxy.json",
    "pay:build": "npm run generate && ng build --project pay --prod --progress=false",
    "pay:analyze": "npm run pay:build -- --stats-json && webpack-bundle-analyzer --port 9201 dist/pay/stats.json",
    "pay:lint": "ng --project pay lint",
    "postinstall": "ngcc"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^10.0.14",
    "@angular/cdk": "^10.1.3",
    "@angular/common": "^10.0.14",
    "@angular/compiler": "^10.0.14",
    "@angular/core": "^10.0.14",
    "@angular/forms": "^10.0.14",
    "@angular/platform-browser": "^10.0.14",
    "@angular/platform-browser-dynamic": "^10.0.14",
    "@angular/router": "^10.0.14",
    "@googlemaps/loader": "^0.0.9",
    "@types/autosize": "^3.0.7",
    "@types/chart.js": "^2.9.24",
    "@types/downloadjs": "^1.4.2",
    "@types/googlemaps": "^3.39.13",
    "@types/lodash-es": "^4.17.3",
    "@types/lscache": "^1.3.0",
    "@types/markerclustererplus": "^2.1.33",
    "@types/rangy": "0.0.33",
    "@zxing/ngx-scanner": "^3.0.1",
    "autosize": "^4.0.2",
    "bootstrap": "~4.4.1",
    "chart.js": "^2.9.3",
    "core-js": "^2.6.11",
    "downloadjs": "^1.4.7",
    "font-awesome": "^4.7.0",
    "hammerjs": "^2.0.8",
    "jquery": "^3.5.1",
    "lodash-es": "^4.17.15",
    "lscache": "^1.3.0",
    "moment-mini-ts": "^2.20.1",
    "ng-event-source": "^1.0.14",
    "ngx-bootstrap": "^5.6.1",
    "ngx-gallery-9": "^1.0.6",
    "popper.js": "^1.16.1",
    "rangy": "^1.3.0",
    "rxjs": "^6.6.2",
    "tslib": "^2.0.1",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1000.8",
    "@angular/cli": "^10.0.8",
    "@angular/compiler-cli": "^10.0.14",
    "@angular/language-service": "^10.0.14",
    "@types/node": "^12.12.54",
    "codelyzer": "^6.0.0",
    "ng-openapi-gen": "^0.12.1",
    "ng-translation-gen": "^0.5.2",
    "svgo": "^1.3.2",
    "tar": "^4.4.13",
    "ts-node": "~7.0.1",
    "tslint": "^6.1.3",
    "typescript": "3.9.7",
    "webpack-bundle-analyzer": "^3.8.0",
    "xmldom": "^0.1.31"
  }
}